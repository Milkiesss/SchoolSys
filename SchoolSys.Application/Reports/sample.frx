<?xml version="1.0" encoding="utf-8"?>
<Report ScriptLanguage="CSharp" ReportInfo.Name="Session Report">
  <Parameter Name="GroupNumber" DataType="String"/>
  <Parameter Name="Date" DataType="String"/>
  <Parameter Name="SubjectName" DataType="String"/>
  <Parameter Name="ExamType" DataType="String"/>
  <Parameter Name="IsIncludedInDiploma" DataType="Boolean"/>
  
  <Dictionary>
    <BusinessObjectDataSource Name="Students" ReferenceName="Students" Enabled="true">
      <Column Name="StudentName" DataType="System.String"/>
      <Column Name="Grade" DataType="System.Int32"/>
      <Column Name="Status" DataType="System.String"/>
    </BusinessObjectDataSource>
  </Dictionary>

  <ReportPage Name="Page1">
    <ReportTitleBand Name="ReportTitle1" Width="718.2" Height="100">
      <TextObject Name="Text1" Left="0" Top="0" Width="300" Height="20" Text="Группа: [GroupNumber]"/>
      <TextObject Name="Text2" Left="0" Top="20" Width="300" Height="20" Text="Дата: [Date]"/>
      <TextObject Name="Text3" Left="0" Top="40" Width="300" Height="20" Text="Предмет: [SubjectName]"/>
      <TextObject Name="Text4" Left="0" Top="60" Width="300" Height="20" Text="Тип экзамена: [ExamType]"/>
    </ReportTitleBand>

    <ColumnHeaderBand Name="ColumnHeader1" Top="100" Width="718.2" Height="20">
      <TextObject Name="Text5" Left="0" Top="0" Width="200" Height="20" Text="Студент" Border.Lines="All"/>
      <TextObject Name="Text6" Left="200" Top="0" Width="100" Height="20" Text="Оценка" Border.Lines="All"/>
      <TextObject Name="Text7" Left="300" Top="0" Width="100" Height="20" Text="Статус" Border.Lines="All"/>
    </ColumnHeaderBand>

    <DataBand Name="Data1" Top="120" Width="718.2" Height="20" DataSource="Students">
      <TextObject Name="Text8" Left="0" Top="0" Width="200" Height="20" Text="[Students.StudentName]" Border.Lines="All"/>
      <TextObject Name="Text9" Left="200" Top="0" Width="100" Height="20" Text="[Students.Grade]" Border.Lines="All"/>
      <TextObject Name="Text10" Left="300" Top="0" Width="100" Height="20" Text="[Students.Status]" Border.Lines="All"/>
    </DataBand>
  </ReportPage>
</Report>